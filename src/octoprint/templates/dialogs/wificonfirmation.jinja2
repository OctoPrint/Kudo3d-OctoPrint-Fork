<div id="wificonfirmation_dialog" class="modal hide fade">
    <div class="modal-header">
        <a href="#" class="close" data-dismiss="modal" aria-hidden="true">&times;</a>
        <h3>{{ _('WiFi Setup') }}</h3>
    </div>
    <!--div class="modal-body"-->
        <div class="row-fluid" style="margin-bottom: 5px">
         <label class="span3" >{{ _('SSID') }}</label>
            <div class="input-append span3">
                <input type="text" class="input-small text-right" data-bind="value: ssid">
            </div>
          <label class="span3">{{ _('PASSWORD') }}</label>
            <div class="input-append span3">
                <input type="text" class="input-small text-right" data-bind="value: password">
            </div>
        </div>
    <div class="modal-footer">
        <a href="#" class="btn" data-dismiss="modal" aria-hidden="true">{{ _('Cancel') }}</a>
        <a href="#" class="btn btn-danger wificonfirmation_dialog_acknowledge">{{ _('Connect') }}</a>
    </div>
</div>
